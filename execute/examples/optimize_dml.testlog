# delete
ok: explain delete customer where person_id = 1
plan                                                                    
Delete customer
	Filter* (Equal customer.person_id 1)
		SeqScan customer

# insert_values
ok: explain insert into person (person_id, first_name, last_name, modified_date) values (1, 'Foo', 'Bar', timestamp '2020-01-01')
plan                                                                                                                                                  
Insert person
	Values person.person_id person.first_name person.last_name person.modified_date [1 "Foo" "Bar" 2020-01-01 00:00:00 UTC]
		TableFreeScan

# update_where
ok: explain update person set first_name = 'Foo' where person_id = 1
plan                                                                                                                                                                                      
Insert person
	Delete person
		Map person.person_id person.first_name:"Foo" person.last_name person.modified_date $tid
			IndexScan person(person_id:1)
					Broadcast
						TableFreeScan

# update_equi_join
ok: explain update customer set account_number = account_number + 1 from person where customer.person_id = person.person_id
plan                                                                                                                                                                                                                                                                                                                       
Insert customer
	Delete customer
		Map customer.customer_id customer.person_id customer.store_id customer.account_number:(AddInt64 customer.account_number 1) customer.modified_date $tid
			HashJoin Inner (Equal customer.person_id person.person_id)
				Exchange
					SeqScan customer
				Exchange
					SeqScan person

